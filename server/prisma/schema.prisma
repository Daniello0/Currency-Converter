generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model defaultTable {
  user_id       Int    @id @unique(map: "user_id_UNIQUE")
  base_currency String @db.TinyText
  favorites     Json
  created_at    String @db.Text
  updated_at    String @db.Text
}
